{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH"
      ]
    },
    {
      "src": "/api/schemas.*",
      "dest": "server.js"
    },
    {
      "src": "/mock/.*",
      "dest": "server.js"
    },
    {
      "src": "/health",
      "dest": "server.js"
    }
  ],
  "env": {
    "PORT":"3000",
    "DB_PATH":"./config/db.json",
    "SCHEMAS_PATH":"./config/schemas.json",
    "API_URL":"/mock",
    "DELAY":"100",
    "FRONTEND_DIST_PATH":"./dist"
  }
}
