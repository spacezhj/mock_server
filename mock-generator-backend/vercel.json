{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH"
      ]
    }
  ],
  "env": {
    "PORT":"8080",
    "DB_PATH":"config/db.json",
    "SCHEMAS_PATH":"config/schemas.json",
    "API_URL":"/mock",
    "DELAY":100,
    "FRONTEND_DIST_PATH":"../mock-generator-frontend-v3/dist"
  }
}
